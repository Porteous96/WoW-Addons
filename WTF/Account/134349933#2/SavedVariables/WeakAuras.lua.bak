					["custom"] = "local units = {}\nlocal t = WeakAuras.triggerState[aura_env.id]\nfor unit, state in pairs(t[1]) do\n    tinsert(units, state.unitName)\nend\ntable.sort(units)\nif tIndexOf(units, aura_env.state.unitName) == aura_env.config.attrib then\n    local frame = aura_env.GetFrame(aura_env.state.unit)\n    if frame then\n        aura_env.Glow(frame, true)\n        aura_env.state.frame = frame\n    end\nend",
